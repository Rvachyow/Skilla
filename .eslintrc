{
    "ignorePatterns": ["const.ts"],
    "extends": [
        "react-app",
        "react-app/jest"
      ],
    "rules": {
        "no-console": "warn",
        "quotes": ["error", "double"],
        "jsx-quotes": ["error", "prefer-double"],
        "comma-dangle": ["error", "always-multiline"],
        "semi": ["warn", "always"],
        // "max-len": ["error", {"code": 140}]
    }
  }